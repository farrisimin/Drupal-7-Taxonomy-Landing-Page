<?php

/**
 * @file
 * Green module installation
 */

/**
 * Implements hook_install().
 */
function green_install() {
  $t = get_t();
  drupal_set_message($t("Green Module Installed."));
}

/**
 * Implements hook_uninstall().
 */
function green_uninstall() {
  $t = get_t();
  drupal_set_message($t("Green Module Un-Installed."));
}
